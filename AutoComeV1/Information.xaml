<Window x:Class="AutoComeV1.Information"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoComeV1"
        mc:Ignorable="d"
        Title="Information" Height="152.055" Width="300">
    <Grid>
        <TextBox VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="72" TextWrapping="Wrap" Text="Actions Completed." VerticalAlignment="Top" Width="213" Margin="36,0,0,0" FontSize="14" BorderBrush="{x:Null}">
            <TextBox.Background>
                <ImageBrush ImageSource="pictures/completed.png"/>
            </TextBox.Background>
        </TextBox>
        <Button x:Name="operateButton" Content="OK" HorizontalAlignment="Left" Margin="153,77,0,0" VerticalAlignment="Top" Width="96" Height="34" Background="#FFBDFF97" Click="OKClick"/>
        <Button x:Name="cancleButton" Content="Undo" HorizontalAlignment="Left" Margin="31,77,0,0" VerticalAlignment="Top" Width="98" Height="34" Background="White" Click="UndoClick"/>


    </Grid>
</Window>
