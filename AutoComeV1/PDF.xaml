<Window x:Class="AutoComeV1.PDF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoComeV1"
        mc:Ignorable="d"
        Title="PDF" Height="500" Width="600">
    <Window.Background>
        <ImageBrush ImageSource="pictures/pdfEmpty.jpg"/>
    </Window.Background>
    <Grid>
        <TextBox x:Name="title" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="96" Margin="23,127,0,0" TextWrapping="Wrap" Text="Title" VerticalAlignment="Top" Width="546" FontSize="22" FontWeight="Bold" FontFamily="Segoe UI Black" />
        <Image x:Name="authors" HorizontalAlignment="Left" Height="100" Margin="23,228,0,0" VerticalAlignment="Top" Width="546"/>
        <TextBox x:Name="_abstract" HorizontalAlignment="Left" Height="136" Margin="23,333,0,0" TextWrapping="Wrap" Text="Abstract" VerticalAlignment="Top" Width="264"/>
        <TextBox x:Name="content" HorizontalAlignment="Left" Height="136" Margin="287,333,0,0" TextWrapping="Wrap" Text="Content " VerticalAlignment="Top" Width="282"/>


    </Grid>
</Window>
